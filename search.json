---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date | date: "%d/%m/%Y" }}",
      "excerpt"  : "{{ post.excerpt | strip_html | strip_newlines }}",
      "author"   : "{{ post.author.name }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]